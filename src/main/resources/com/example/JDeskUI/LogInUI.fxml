            <Label prefHeight="24.0" prefWidth="153.0" text="Username" />
            <TextField fx:id="LoginUsernameField" prefHeight="34.0" prefWidth="225.0" promptText="Username" />
            <Label prefHeight="27.0" prefWidth="122.0" text="Password" />
            <TextField fx:id="LoginPasswordField" prefHeight="38.0" prefWidth="225.0" promptText="Password">
